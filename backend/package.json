{
  "name": "olimpos",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --testEnvironment=node --verbose --forceExit --watchAll --maxWorkers=1",
    "test-coverage": " jest --collect-coverage --testEnvironment=node --forceExit --maxWorkers=1",
    "lint": "lint-staged",
    "lint-all": "pretty-quick && eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "author": "Hasan Ekiz",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "helmet": "^5.1.0",
    "http-errors": "^2.0.0",
    "mongoose": "^6.3.3",
    "mongoose-autopopulate": "^0.16.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.3",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.0",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^28.1.0",
    "lint-staged": "^10.2.11",
    "livereload": "^0.9.1",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1",
    "supertest": "^6.2.3"
  }
}
